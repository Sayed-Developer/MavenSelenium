<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Happy Scenario to Register Fine" verbose="1" >
    <test name="Immediate Fine Registration">
        <classes>
            <class name="OpenAndCloseBrowser">
                <methods>
                    <include name="OpenAndCloseBrowser"/>
                </methods>
            </class>
            <class name="LoginAndLogout">
                <methods>
                    <include name="loginWithValidData"/>
                    <include name="selectValidProsecution"/>
                </methods>
            </class>
            <class name="ImmediateFineRegistration">
                <methods>
                    <include name="registerFine"/>
                </methods>
            </class>
            <class name="PayingLicenseFines">
                <methods>
                    <include name="payingLicFines"/>
                    <include name="payPOSLicFines"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>

